    "@kurone-kito/jsonresume-types": "^0.2.5",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/tailwindcss": "^3.0.10",
    "autoprefixer": "^10.4.4",
